{
  "name": "template-vite-react-ts-sass",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "pre-commit": "lint-staged",
    "preview": "vite preview",
    "prepare": "husky install",
    "type-check": "tsc --noEmit --skipLibCheck",
    "clear-buildinfo": "rm tsconfig.tsbuildinfo",
    "pre:push": "yarn type-check; yarn clear-buildinfo"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "ws": "8.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/babel__core": "^7.1.19",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.10",
    "@types/ws": "8.5.3",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "@vitejs/plugin-react": "1.0.7",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.4",
    "eslint": "8.12.0",
    "eslint-plugin-react": "7.29.4",
    "husky": "7.0.0",
    "lint-staged": "12.3.7",
    "prettier": "2.6.2",
    "sass": "1.49.11",
    "typescript": "4.5.4",
    "vite": "2.9.0"
  }
}
